import java.util.Scanner;

public class ArrayEj1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		final int n = 10;
		int Array1 [];
		Array1 = new int[n];
		int i, contaMay=0, contaMin=0;
		System.out.println("Escribe una frase");
		for (i=0; i <= n; i++){
			Array1[i] = sc.nextInt();
		}
		
	

	}//main

}//Class
