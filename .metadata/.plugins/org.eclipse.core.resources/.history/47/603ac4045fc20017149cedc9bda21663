import java.util.Scanner;

public class Hoja2Ej5 {
//MAL
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s1,s2;
		int i;
		System.out.println("Introduce una frase: ");
		s1 = sc.nextLine();
		int ultimo = s1.charAt(s1.length()-1);
		int primero = s1.charAt(1);
		System.out.println(s1.substring(s1.length()-1, s1.charAt(1)));	

	}//main
	
}//Class