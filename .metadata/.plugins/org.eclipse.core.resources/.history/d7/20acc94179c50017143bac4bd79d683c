public class Pseudocodigo3_37v2 {

	public static void main(String[] args) {
		final int F = 4;
		final int C = 5;
		int matriz [][], filas, columnas; 
		int mT[][]=new int[5][4];
		matriz = new int[F][C];
		//System.out.println("\tMatriz normal (4x5)");

		//System.out.println("\n"); //2 saltos de linea
		//System.out.println("\tMatriz transpuesta (5x4)");
		for (filas=0; filas<F; filas++){ //se recorre por columnas
			System.out.println();
			for (columnas=0; columnas<C; columnas++){
				matriz [F][C]=mT[C][F]=(int)Math.floor(Math.random()*101+1);
			}
		}

	}//main

}//Class
