public class Pseudocodigo3_40 {

	public static void main(String[] args) {
		int i,j,k, conta=1;
		int tabla [][][] = new int[3][4][5];
		for (i=0; i<3; i++){
			System.out.println();
			System.out.println("--------------------------");
			for (j=0; j<4; j++){
				System.out.println();
				for (k=0; k<5; k++){
					tabla[i][j][k]=conta;
					conta++;
					System.out.printf("%5d", tabla[i][j][k]);
				}
			}
		}
		System.out.println("\n--------------------------");
	}//main

}//Class
