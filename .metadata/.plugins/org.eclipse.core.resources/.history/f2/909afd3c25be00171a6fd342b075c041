import java.util.Scanner;

public class ArrayEj9 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int i,n;
		String cartaNum [] = {"1", "2", "3", "4", "5", "6", "7"};
		String palo [] = {"O", "C", "E", "B"};
		String cartaFig [] = {"A","B","C"};
		String carta="";
		System.out.println("Pulsa 0 para salir");
		n = sc.nextInt();
		int num=(int)Math.floor(Math.random()*6+1);
		while (n!=0){
			for (i=0; i<52; i++){
				System.out.println(cartaNum.length-num+palo[i]);
				
			}
			System.out.println("Pulsa 0 para salir");
			n = sc.nextInt();
		}


	}//main

}//Class
