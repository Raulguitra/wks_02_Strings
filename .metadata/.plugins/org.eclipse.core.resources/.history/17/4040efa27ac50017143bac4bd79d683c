import java.util.Scanner;

public class Pseudocodigo3_38 {

	public static void main(String[] args) {
		final int alumno = 300;
		final int asig = 10;
		int Array1 [][],i,j, numFila,conta=0;
		Array1 = new int[alumno][asig];
		Scanner sc = new Scanner(System.in);
		for (i=0;i<alumno; i++){
			System.out.println();
			for (j=0; j<asig; j++){
				Array1 [i][j]= (int)Math.floor(Math.random()*10);
				System.out.printf("%9d", Array1 [i][j]);
			}
		}

	}//main

}//Class
