
public class ArrayEj7 {

	public static void main(String[] args) {
		final int f = 10;
		final int c = 10;
		int Array1 [][];
		Array1 = new int[f][c];
		int i,j=0, sumaF=0, sumaC=0;
		for (i=0; i<f; i++){
			for (j=0; j<c; j++){
				Array1[i][j]=(int)Math.floor(Math.random()*21-10);
				System.out.println(Array1[i][j] +"["+i+"]" +"["+j+"]");
			}
		}

	}//main

}//Class
