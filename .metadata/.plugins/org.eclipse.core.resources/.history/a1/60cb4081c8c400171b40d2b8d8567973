public class Pseudocodigo3_37 {

	public static void main(String[] args) {
		final int F = 4;
		final int C = 5;
		int matriz [][], filas, columnas;
		matriz = new int[F][C];
		for (filas=0; filas<F; filas++){
			System.out.println("\t\t\t");
			for (columnas=0; columnas<C; columnas++){
				matriz [filas][columnas]=(int)Math.floor(Math.random()*(101)); //generar matriz 4x5 con nums entre 1y100
				System.out.printf("%5d", matriz [filas][columnas]);
			}
			
		}

	}//main

}//Class
