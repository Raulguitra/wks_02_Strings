import java.util.Scanner;

public class Pseudocodigo3_38 {

	public static void main(String[] args) {
		final int alumno = 10;
		final int asig = 10;
		int Array1 [][],i,j, numFila,conta=0;
		Array1 = new int[alumno][asig];
		Scanner sc = new Scanner(System.in);
		for (i=0;i<alumno; i++){
			for (j=0; j<asig; j++){
				Array1 [i][j]= sc.nextInt();
			}
		}

	}//main

}//Class
