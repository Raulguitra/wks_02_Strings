import java.util.*;
public class Pseudocodigo3_33 {

	public static void main(String[] args) {
		int i, conta=0;
		Scanner sc = new Scanner(System.in);
		String arrayNombres[] = new String[3];
		System.out.println("Introduce 3 nombres");
		for (i=0; i<arrayNombres.length; i++){
			if (conta!=0){
				System.out.println("Introduce otro nombre");
			}
			conta++;
			arrayNombres[i] = sc.nextLine();
		}
		for (i=0; i<arrayNombres.length; i++){
			System.out.println(arrayNombres[i]);
		}

	}//main

}//Class
